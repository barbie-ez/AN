services:
  an:
    build:
      context: /Users/barbaraezomo/Projects/AN
      dockerfile: AN/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: AN
      target: base
    container_name: AN
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: an:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp3.1/AN.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - /Users/barbaraezomo/Projects/AN/AN:/app:rw
    - /Users/barbaraezomo/vsdbg/vs2017u5:/remote_debugger:rw
    - /Users/barbaraezomo/.aspnet/https:/root/.aspnet/https:ro
    - /Users/barbaraezomo/.microsoft/usersecrets:/root/.microsoft/usersecrets:ro
    - /usr/local/share/dotnet/sdk/NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - /Users/barbaraezomo/.nuget/packages:/root/.nuget/packages:ro
    - /Users/barbaraezomo/Projects/AN:/src:rw
version: '3.4'